// в строку мы можем засунуть, сколько угодно символов
var a: String  = "Salaam"
// в char мы можем засунуть только один символ
var b : Char = "a"

// для создания пустой строки ее нужно инициализировать, а также присвоить ей пустую строку.

var str: String = ""
print(str)


// усправляющие последовательность
// \n: перевод на новую строку

// \t: табуляция

// \": кавычка

// \\: обратный слеш

// здесь мы выводим кавычки
print("\"Hello, World!\"")


// здесь мы выводим sal
// am
//

print("sal\nam")
